package RunningThroughXML_part4_parallelExecution;

import org.testng.annotations.Test;

public class Parallel_classes {

	
	@Test
	public void method1() throws InterruptedException
	{
		for (int i =0;i<=5;i++)
		
		{
			
			System.out.println( (i));
			Thread.sleep(2000);
			
		}
		
	}

	
	

}
